<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <title>Admin - Concours</title>
    <link href="{{data.asset_path}}/css/bootstrap.min.css" rel="stylesheet">
    <link href="{{data.asset_path}}/css/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css">
    <link href="{{data.asset_path}}/css/sb-admin-2.min.css" rel="stylesheet">
    <link href="{{data.asset_path}}/css/index.css" rel="stylesheet">
    <script>
			(function(d, s, id) {
				var js, fjs = d.getElementsByTagName(s)[0];
				if (d.getElementById(id)) return;
				js = d.createElement(s); js.id = id;
				js.src = "//connect.facebook.net/en_US/sdk.js";
				fjs.parentNode.insertBefore(js, fjs);
			}(document, 'script', 'facebook-jssdk'));
	</script>
</head>

<body>
    <div id="wrapper">
        <!-- Navigation -->
        <nav class="navbar navbar-inverse navbar-static-top navbar-surcharge" role="navigation">
            <div class="container">
                <!-- Burger Mobile -->
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <a class="navbar-brand" href="/admin/{{controller.getID()}}/config">Admin</a>
                </div>
                <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                    <ul class="nav navbar-nav">
                        <li>
                            <a href="/">Concours</a>
                        </li>
                        <li>
                            <a>Comment jouer</a>
                        </li>
                    </ul>
                </div>
                <!-- /.Burger Mobile -->
            </div>
        </nav>
        <!-- /.Navigation -->
        <!-- /#wrapper -->
        <div id="page-wrapper">
            {% block content %}{% endblock %}
        </div>
        <footer>
            {% block footer %}{% endblock %}
        </footer>
        <script
  src="https://code.jquery.com/jquery-3.1.1.min.js"
  integrity="sha256-hVVnYaiADRTO2PzUGmuLJr8BLUSjGIZsDYGmIJLv2b8="
  crossorigin="anonymous"></script>
        <script src="{{data.asset_path}}/script/bootstrap.min.js"></script>
        <!-- Custom JavaScript -->
        <script src="{{data.asset_path}}/script/third-party/sb-admin-2.min.js"></script>
        <script src="{{data.asset_path}}/script/third-party/color.js"></script>
        <script src="{{data.asset_path}}/script/third-party/colorPicker.data.js"></script>
        <script src="{{data.asset_path}}/script/third-party/colorPicker.js"></script>
        <script src="{{data.asset_path}}/script/third-party/jsColor.js"></script>
</body>
{% block javascript %}{% endblock %}

</html>